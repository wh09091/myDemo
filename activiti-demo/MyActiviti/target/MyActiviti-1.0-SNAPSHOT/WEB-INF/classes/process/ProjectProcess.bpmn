    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow10" sourceRef="usertask5" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${bossApproved}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow11" name="撤回申请" sourceRef="exclusivegateway1" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!staffApproved}]]></conditionExpression>
